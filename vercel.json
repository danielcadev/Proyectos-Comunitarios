{
    "version": 2,
    "builds": [
      {
        "src": "config/functions/bootstrap.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "config/functions/bootstrap.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }